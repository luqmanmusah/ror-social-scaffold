<section class="users-section">
  <ul class="users-list">
    <%= render @users %> 
  </ul>
</section>
 <% @users.each do |user| %>
<%= user.name %>

 <% if signed_in? && !Friendship.reacted?(current_user.id, user.id) && current_user != user %>
          <%= button_to "Send Invitation",
                        friendships_create_path(ids: { id1: current_user.id, id2: user.id }) %>
<% end %>
<% end %>